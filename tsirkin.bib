@article{ iprJETP,
Author = {Tsirkin, Stepan S and Chulkov, E. V.},
Title = {{Image Potential Eigenstates and Resonances on the (110) Surfaces of
   Noble Metals: Energies and Lifetimes}},
Journal = {{JETP}},
Year = {{2014}},
Volume = {{118}},
Number = {{2}},
Pages = {{167-175}},
Month = {{FEB}},
Abstract = {{The energies and lifetimes of the image potential resonances at the (Y)
   over bar point on the Cu(110), Ag(110), and Au(110) surfaces are
   studied, and the energies of the image potential states on the Pd(110)
   surface are analyzed. It is shown that every quantum number n
   corresponds to a pair of image potential states (resonances) n(+) and
   n(-) at the (Y) over bar point. The average energy of a pair of
   eigenstates (resonances) at n >= 2 is well described by the formula (E)
   over bar (n) = (En+ + En-) = E-0 - (0.85 eV)/(n + delta)(2), where delta
   is the quantum defect, E-0 = (1/2m(e))((h) over bar pi/a)(2), and a is
   the lattice parameter. The splitting energy of a pair of eigenstates
   (resonances) obeys the law Delta E-n = En+ - En- proportional to n(-3).
   The lifetimes t(n +/-) of image potential resonances are proportional to
   n(3).}},
DOI = {{10.1134/S1063776114010191}},
