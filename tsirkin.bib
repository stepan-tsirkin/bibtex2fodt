	Id = {Guguchia2020},
	Isbn = {2041-1723},
	Journal = {Nature Communications},
	Number = {1},
	Pages = {559},
	Title = {Tunable anomalous Hall conductivity through volume-wise magnetic competition in a topological kagome magnet},
	Ty = {JOUR},
